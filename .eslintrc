{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": false
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "globals": {
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": "warn",
    "no-redeclare": "warn",
    "no-undef": "error",
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        }
      }
    ]
  }
}