{
  "name": "speak-r",
  "version": "0.0.3",
  "description": "Functional programming in human-readable form",
  "main": "dist/speak-r.js",
  "jsnext:main": "dist/speak-r.jsnext.js",
  "scripts": {
    "test": "nodemon test/index.js -debug | tap-spec",
    "start": "nodemon index.js",
    "start-debug": "set DEBUG=tree:*,index:* & nodemon app/index.js",
    "build": "rollup -c & rollup -c --environment BUILD:es5"
  },
  "keywords": [
    "fp",
    "functional",
    "ramda"
  ],
  "author": "Zero Bias",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.22.1",
    "sanctuary": "^0.11.1",
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "rollup": "^0.35.6",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-cleanup": "^0.1.2",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3"
  }
}
